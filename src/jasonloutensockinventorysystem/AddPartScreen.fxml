<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="357.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jasonloutensockinventorysystem.AddPartScreenController">
   <children>
      <HBox prefHeight="51.0" prefWidth="410.0">
         <children>
            <Label text="Add Part" textFill="#180286">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <RadioButton fx:id="inHouse" mnemonicParsing="false" onAction="#radioButtonChanged" selected="true" text="In-House">
               <HBox.margin>
                  <Insets left="50.0" top="20.0" />
               </HBox.margin>
            </RadioButton>
            <RadioButton fx:id="outsourced" mnemonicParsing="false" onAction="#radioButtonChanged" text="Outsourced">
               <HBox.margin>
                  <Insets left="50.0" top="20.0" />
               </HBox.margin>
            </RadioButton>
         </children>
      </HBox>
      <VBox layoutX="42.0" layoutY="51.0" prefHeight="235.0" prefWidth="97.0">
         <children>
            <Label text="ID">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label text="Name">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label text="Inv">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label text="Price/Cost">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label text="Max">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="machineIDLabel" onDragDetected="#radioButtonChanged" prefHeight="17.0" prefWidth="97.0" text="Machine ID">
               <VBox.margin>
                  <Insets top="24.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="135.0" layoutY="51.0" prefHeight="157.0" prefWidth="135.0">
         <children>
            <TextField fx:id="partIDTextField" disable="true" promptText="Auto Gen - Disabled">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <TextField fx:id="nameTextField" promptText="Part Name">
               <VBox.margin>
                  <Insets top="14.0" />
               </VBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <TextField fx:id="invTextField" promptText="Inv">
               <VBox.margin>
                  <Insets top="14.0" />
               </VBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <TextField fx:id="priceTextField" promptText="Price/Cost">
               <VBox.margin>
                  <Insets top="12.0" />
               </VBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </VBox>
      <HBox layoutX="135.0" layoutY="208.0" prefHeight="40.0" prefWidth="262.0">
         <children>
            <TextField fx:id="maxTextField" prefHeight="25.0" prefWidth="65.0" promptText="Max">
               <HBox.margin>
                  <Insets top="8.0" />
               </HBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <Label text="Min">
               <HBox.margin>
                  <Insets left="40.0" top="11.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="minTextField" prefHeight="25.0" prefWidth="65.0" promptText="Min">
               <HBox.margin>
                  <Insets left="20.0" top="8.0" />
               </HBox.margin>
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <TextField fx:id="machineIDTextField" layoutX="146.0" layoutY="255.0" onAction="#radioButtonChanged" prefHeight="25.0" prefWidth="125.0" promptText="Match ID">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <HBox layoutX="132.0" layoutY="305.0" prefHeight="40.0" prefWidth="273.0">
         <children>
            <Button fx:id="addPartSaveButton" mnemonicParsing="false" onAction="#addPartSaveButtonActionEvent" prefHeight="30.0" prefWidth="80.0" text="Save">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonActionEvent" prefHeight="30.0" prefWidth="80.0" text="Cancel">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
